#!name=JD
#!desc=Safari浏览器打开登录 https://bean.m.jd.com ,点击签到并且出现签到日历后, 如果通知获得cookie成功, 则可以使用此签到脚本。
#!system=ios

[Script]
# > 京东京豆
http-request https:\/\/api\.m\.jd\.com\/client\.action.*functionId=signBean max-size=0,script-path=https://raw.githubusercontent.com/NobyDa/Script/master/JD-DailyBonus/JD_DailyBonus.js
cron “5 0 * * *” script-path=https://raw.githubusercontent.com/NobyDa/Script/master/JD-DailyBonus/JD_DailyBonus.js
# > 京东比价
http-response ^https?://api\.m\.jd\.com/client\.action\?functionId=(wareBusiness|serverConfig|basicConfig) requires-body=1,script-path=https://raw.githubusercontent.com/yichahucha/surge/master/jd_price.js
# > 京小超
cron “11 1-23/5 * * *”,wake-system=1,timeout=120,script-path=https://raw.githubusercontent.com/lxk0301/scripts/master/jd_superMarket.js
# > 京东摇钱树
cron “3 */2 * * *”,wake-system=1,timeout=20,script-path=https://raw.githubusercontent.com/lxk0301/scripts/master/jd_moneyTree.js
# > 京东宠汪汪
cron “15 */2 * * *”,wake-system=1,timeout=20,script-path=https://raw.githubusercontent.com/lxk0301/scripts/master/jd_joy.js
# > 京东天天加速
cron “8 */3 * * *”,wake-system=1,timeout=20,script-path=https://raw.githubusercontent.com/lxk0301/scripts/master/jd_speed.js
# > 京东东东农场
cron “5 6-18/6 * * *”,wake-system=1,timeout=20,script-path=https://raw.githubusercontent.com/lxk0301/scripts/master/jd_joy_steal.js
# > 京东东东萌宠
cron “5 6-18/6 * * *”,wake-system=1,timeout=20,script-path=https://raw.githubusercontent.com/lxk0301/scripts/master/jd_pet.js
# > 京东种豆得豆
cron “1 7-21/2 * * *”,wake-system=1,timeout=20,script-path=https://raw.githubusercontent.com/lxk0301/scripts/master/jd_plantBean.js
# > 取关京东店铺商品
cron “55 23 * * *”,wake-system=1,timeout=20,script-path=https://raw.githubusercontent.com/ddgksf2013/Cuttlefish/master/Task/jd_unsubscribe.js

[MITM]
hostname = %APPEND% api.m.jd.com

